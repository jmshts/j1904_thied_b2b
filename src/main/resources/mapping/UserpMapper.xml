  <!--  查询用户的权限信息-->
  <select id="selectPermissionByUName" resultType="com.sy.pojo.Permission" parameterType="string">
        select
            p.*
        from
            userp                    u,
            role                        r,
            permission                  p,
            user_role_mapping           ur,
            role_permission_mapping     rp
            where
            u.uid = ur.uid
            and ur.rid = r.roleId
            and r.roleId = rp.rid
            and rp.pid = p.permissionId
            and u.uname = #{uname}
