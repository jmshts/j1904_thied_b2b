  <!--  根据用户名查询角色信息-->
  <select id="selectRoleByUName" parameterType="string" resultType="com.sy.pojo.Role">
      SELECT r.*
        FROM
         userp        u,
         role        r,
         user_role_mapping       ur
          WHERE
          u.uid = ur.uid
          AND ur.rid = r.roleId
          AND u.uname = #{uname}
  </select>
